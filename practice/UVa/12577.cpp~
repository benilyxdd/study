#include <bits/stdc++.h>
using namespace std;

#define ll long long

void solve() {
	string s;
	int i = 1;
	while(cin >> s) {
		if (s == "*") 
			break;

		cout << "Case " << i << ": ";
		cout << (s == "Hajj" ? "Hajj-e-Akbar" : "Hajj-e-Asghar") << "\n";

		i++;
	}
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	//cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}



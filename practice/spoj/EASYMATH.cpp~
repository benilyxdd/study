#include <bits/stdc++.h>
using namespace std;

#define ll long long

void solve() {
	int n, m, a, d;
	cin >> n >> m >> a >> d;

	int cnt = 0;
	cnt += (a >= n && a <= m);
	cout << cnt;
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define int ll
const int MAX = (int)1e18+9;

void solve() {
	int n, m, k;
	cin >> n >> m >> k;
	vector<int> a, b;
	for (int i = 0, x; i < n; i++) {
		cin >> x;
		a.push_back(x);
	}
	for (int i = 0, x; i < m; i++) {
		cin >> x;
		b.push_back(x);
	}
	for (int i = 0; i < max(n, m)-n; i++) {
		a.push_back(MAX);
	}
	for (int i = 0; i < max(n, m)-m; i++) {
		b.push_back(MAX);
	}

	int i = 0, j = 0, cnt = 0;
	while(true) {
		if (a[i] > k && b[j] > k || i > n-1 && j > m-1) {
			break;
		}

		int mn = min(a[i], b[j]);
		k -= mn;
		cnt++;
		if (mn == a[i]) {
			i++;
		} else {
			j++;
		}
	}
	cout << cnt;
}

signed main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	//cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


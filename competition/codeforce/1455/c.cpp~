#include <bits/stdc++.h>
using namespace std;

#define ll long long

void solve() {
	int a, b;
	cin >> a >> b;
	int mn = min(a, b)-1;
	a -= mn;
	b -= mn;
	if (a == 1) {
		cout << "0 " << b << "\n";
	} else if (b == 1) {
		cout << a-1 << " 1\n";
	}
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


#include <bits/stdc++.h>
using namespace std;

#define ll long long
const int MOD = (int)1e9+7, mxN = (int)5e5+5;
string ar[mxN];
int n;

void solve() {
	cin >> n;
	for (int i = 0; i < n; i++) 
		cin >> ar[i];

	for (int i = 0; i < n; i++) 
		cout << ar[i];
	cout << "\n";
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


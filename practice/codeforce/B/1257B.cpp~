#include <bits/stdc++.h>

using namespace std;

#define ll long long

void solve() {
	int x, y;
	cin >> x >> y;
	if (x > 3) {
		cout << "YES\n";
	} else if (x == 1) {
		if (y == 1) {
			cout << "YES\n";
		} else {
			cout << "NO\n";
		}
	} else {
		if (y <= 3) {
			cout << "Yes\n";
		} else {
			cout << "NO\n";
		}
	}
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


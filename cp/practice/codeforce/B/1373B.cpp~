#include <bits/stdc++.h>

using namespace std;

#define ll long long

void solve() {
	int a = 0, b = 0;
	string s;
	cin >> s;
	for (int i = 0; i < s.length(); i++) {
		if (s[i] == '0') {
			a++;
		} else {
			b++;
		}
	}
	int mn = min(a, b);
	cout << (mn % 2 ? "DA" : "NET") << "\n";
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		//cout << "Case #" << i < ": ";
		solve();
		//i++;
	}
	return 0;
}

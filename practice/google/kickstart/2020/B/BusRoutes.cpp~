#include <bits/stdc++.h>

using namespace std;

#define ll long long

void solve() {
	ll a, b;
	cin >> a >> b;
	int mn = 1001;
	vector<int> v;
	int c;
	for (int i = 0; i < a; i++) {
		cin >> c;
		v.push_back(c);
	}
	for (int i = a-1; i >= 0; i--) {
		ll x = b/v[i];
		ll xd = x*v[i];
		if (xd > mn) {
		    ll diff = xd-mn;
		    ll times = (diff+v[i]-1)/v[i];
		    xd -= times*v[i];
		}
		mn = min((ll)mn, xd);

	}
	cout << mn << "\n";
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		cout << "Case #" << i << ": ";
		solve();
		i++;
	}
	return 0;
}


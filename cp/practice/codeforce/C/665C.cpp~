#include <bits/stdc++.h>

using namespace std;

#define ll long long

void solve() {
	string s;
	cin >> s;
	int len = s.length();
	for (int i = 1; i < len; i++) {
		if (s[i] == s[i-1]) {
			if (s[i] != 'a' && s[i+1] != 'a') {
				s[i] = 'a';
			} else if (s[i+1] != 'b') {
				s[i] = 'b';
			} else {
				s[i] = 'c';
			}
		}
	}
	cout << s << "\n";
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	//cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


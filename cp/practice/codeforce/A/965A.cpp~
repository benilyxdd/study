#include <bits/stdc++.h>

using namespace std;

#define ll long long

void solve() {
	int k, n, s, p;
	cin >> k >> n >> s >> p;
	int x = (n+s-1)/s;
	int y = x*k;
	cout << (y+p-1)/p;
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	//cin >> t;
	while(t--) {
		//cout << "Case #" << i < ": ";
		solve();
		//i++;
	}
	return 0;
}

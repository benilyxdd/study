#include <bits/stdc++.h>
using namespace std;

#define ll long long
#define int ll
const int mxN = (int)2e5+5;
int ar[mxN], n, k;

void solve() {
	cin >> n >> k;
	assert(k == 0);
	for (int i = 0; i < n; i++)
		cin >> ar[i];

	int ans = 0;
	set<int> s;
	for (int i = 0; i < n; i++) {
		int x = ar[i];
		if (s.size() == 0) {
			s.insert(x);
			continue;
		}
		for (const int& y : s) {
			int t = x*y;
			int t2 = sqrt(t);
			if (t2*t2 == t) {
				ans++;
				s.clear();
				s.insert(x);
				continue;
			}
		}
		s.insert(x);
	}
	cout << ans << '\n';
}

signed main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


#include <bits/stdc++.h>
using namespace std;

#define ll long long

void solve() {
	int a, b;
	cin >> a >> b;
	int x = a*b;
	int cnt = 0;
	string s = bitset<32>(x).to_string();
	for (int i = 0; i < 32; i++) {
		if (s[i] == '1') {
			cnt++;
		}
	}
	cout << cnt-1 << "\n";
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	//cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


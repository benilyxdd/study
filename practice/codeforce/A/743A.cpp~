#include <bits/stdc++.h>
using namespace std;

#define ll long long
int n, a, b;
string s;

void solve() {
	cin >> n >> a >> b >> s;
	a--, b--;
	cout << ((s[a]-'0')^(s[b]-'0'));
}

int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);

	int t = 1, i = 1;
	//cin >> t;
	while(t--) {
		//cout << "Case #" << i << ": ";
		solve();
		//i++;
	}
	return 0;
}


